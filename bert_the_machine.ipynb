    "    n_components = tuning_comparisons_df.iloc[0]['PCA_n_components']\n",
    "    n_clusters = tuning_comparisons_df.iloc[0]['n_clusters']\n",
    "    n_init = tuning_comparisons_df.iloc[0]['n_init']\n",
    "    max_iter = tuning_comparisons_df.iloc[0]['max_iter']\n",
